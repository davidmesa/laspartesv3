RewriteEngine on

RewriteCond %{HTTP_HOST} !^www\.
RewriteRule ^(.*)$ http://www.%{HTTP_HOST}/$1 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /index.php?$1 [L]

Redirect 301 //hsphere/local/home/laspartes/laspartes.com http://www.laspartes.com/index.php

RedirectMatch 301 ^/co/(.*)$ http://www.laspartes.com/$1

ErrorDocument 404 http://www.laspartes.com/pagina_no_encontrada/